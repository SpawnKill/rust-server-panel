{
  "name": "rust-server-panel",
  "version": "1.0.0",
  "description": "Web панель управления сервером Rust",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup.js",
    "install-rust": "bash scripts/install-rust.sh",
    "install-steamcmd": "bash scripts/install-steamcmd.sh",
    "init-db": "node scripts/init-db.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t rust-panel .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "multer": "^1.4.5-lts.1",
    "chokidar": "^3.5.3",
    "node-pty": "^1.0.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "rcon-client": "^4.3.1",
    "ssh2": "^1.14.0",
    "node-cron": "^3.0.3",
    "winston": "^3.10.0",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "express-validator": "^7.0.1",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "node-fetch": "^3.3.1",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}